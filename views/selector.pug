extends layout

block content
  h1= title
    - var image_names= ["guitar.jpg","keyboard.jpg","flute.jpg","mouthorgan.jpg","violin.jpg"]            
        -for(let i=0;i<3;i++){
            - let rand=Math.floor(Math.random()*5)
            img(src= `/images/${image_names[rand]}` alt="image", height="300", width="300" class="margin-top")
        -}